{
  "name": "Zamiana magazynów dla serwisu idosell",
  "description": "Mam sam zmieniać magazyn z M1 na M0 i odwrotnie",
  "version": "1.0",
  "permissions": [ 
    "https://zooland.com.de/panel/orderd*", "tabs"],
  "manifest_version": 2, 
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {  
    "default_icon": "128.png",  
    "default_popup": "popup.html"  
  }, 
  "content_scripts": [  
    {  
        "matches": [  
          "https://zooland.com.de/panel/orderd*"
          ],  
       "js": ["m1_to_m0.js"]  
        }  
    ]  
}